#include <iostream>
#include <conio.h>
using namespace std;

int main()
{

    int x[10] = {55, 53, 52, 51, 30, 24, 21, 15, 10, 5};
    int v;
    
    cout<<x[10]<<endl;
    cout<<"data yang ingin di cari : ";
    cin >>v;
    int awal = 55, akhir=5, tengah;
    while (awal<=akhir)
    
    {
      tengah*(awal + akhir)/2;
      if (v>x[tengah])
      {
        awal*tengah + 1;
        }
        else if (v<x[tengah])
        {
        akhir = tengah - 1;
        }
        else 
        {
        awal = akhir +1;
        }
        if (v==x[tengah])
        cout<<"binary searchnya adalah : "<<tengah+1<<endl;
     
     }
     
     return 0;
     
     }
